rdens/src/components/AmenitiesComponent.js",[],"/Users/mbuguamwaura/Desktop/projects/sagret-gardens/src/components/AmenitiesListComponent.js",[],"/Users/mbuguamwaura/Desktop/projects/sagret-gardens/src/components/GalleryComponent.js",["98","99","100","101","102","103","104","105","106","107"],"/Users/mbuguamwaura/Desktop/projects/sagret-gardens/src/data/gallery.js",[],"/Users/mbuguamwaura/Desktop/projects/sagret-gardens/src/components/FooterComponent.js",["108"],"import React, { Component } from 'react';\nimport Image from 'react-bootstrap/Image'\nimport './navbar.css'\nimport icons from '../data/icons'\nimport { Row, Col } from 'react-bootstrap'\n\nclass BannerComponent extends Component {\n\n\n\n    render() {\n        const socialIcons = []\n\n        const instagram = { link: \"https://www.instagram.com/sagretgardens/\", icon: \"instagram.png\" }\n        socialIcons.push(instagram)\n\n        const facebook = { link: \"https://www.facebook.com/sagretgardens/\", icon: \"facebook.png\" }\n        socialIcons.push(facebook)\n\n\n        return (\n            <div className=\"footer-container\">\n\n\n                <div style={{ padding: \"2vw 0 5px 0\" }}>\n\n                    <div style={{ padding: \"2vw 7vw\" }}>\n                        <div style={{ textAlign: \"initial\" }}>\n                            <p style={{ fontFamily: \"Optima\", color: \"#ffffff\", fontSize: \"14px\", fontWeight: \"bold\", marginBottom: \"4px\" }}>\n                                We would love to hear from you!\n                                </p>\n                            <p style={{ fontFamily: \"Nunito\", color: \"#ffffff\", fontSize: \"11px\" }}>\n                                To get in touch, please contact us at:\n                                </p>\n                        </div>\n\n                        <Row style={{ textAlign: \"initial\" }}>\n                            <Col className=\"col-6\">\n\n                                {icons.map((icon, i) => (\n\n 